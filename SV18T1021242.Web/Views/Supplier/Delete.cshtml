
@{
    ViewBag.Title = "Xóa nhà cung cấp";
}

<div class="box box-primary">
    <div class="box-header with-border">

    </div>
    <div class="box-body">
        <form class="form-horizontal" action="#">
            <div class="form-group">
                <label class="control-label col-sm-2">Tên Nhà Cung Cấp</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Trương Đặng Ngọc Phúc</p>

                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Tên Giao Dịch</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Giao dịch 1</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Địa Chỉ</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Phú Mậu</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Quốc gia</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Việt Nam</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Thành Phố</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Huế</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Mã Bưu chính</label>
                <div class="col-sm-10">
                    <p class="form-control-static">320000</p>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Số điện thoại</label>
                <div class="col-sm-10">
                    <p class="form-control-static">(+84)836323309</p>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-danger"><i class="fa fa-trash" style="margin-right:5px"></i>Đồng ý xóa</button>
                    <a href="~/customer" class="btn btn-default"><i class="fa fa-chevron-left" style="margin-right:5px"></i>Quay lại</a>
                </div>
            </div>
        </form>
    </div>

</div>



