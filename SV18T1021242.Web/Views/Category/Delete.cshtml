
@{
    ViewBag.Title = "Xóa loại hàng";
}

<div class="box box-primary">
    <div class="box-header with-border">

    </div>
    <div class="box-body">
        <form class="form-horizontal" action="#">
            <div class="form-group">
                <label class="control-label col-sm-2">Tên Loại Hàng</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Nước ngọt</p>

                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Miêu Tả</label>
                <div class="col-sm-10">
                    <p class="form-control-static">Coca</p>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-danger"><i class="fa fa-trash" style="margin-right:5px"></i>Đồng ý xóa</button>
                    <a href="~/category" class="btn btn-default"><i class="fa fa-chevron-left" style="margin-right:5px"></i>Quay lại</a>
                </div>
            </div>
        </form>
    </div>

</div>

